{
    "body":{
        "background-color":"#444",
        "font-family":"arial",
        "color":"#ddd",
        "margin":"0px"
    },
    "p":{
        "margin":"16px 0"
    },
    "a":{
        "cursor":"pointer",
        "color":"#222"
    },
    "a.active,a .active,a:hover":{
        "cursor":"pointer",
        "color":"#666"
    },
    "img":{
        "max-width":"100%"
    },
    "h1,h2,h3,h4,h5":{
        "margin":"16px 0"
    },
    "input,textarea,select":{
        "transition":"background 0.3s ease",
        "font-size":"small",
        "padding":"3px 5px",
        "background-color":"#ddd4",
        "border":"1px solid #bbb"
    },
    "input:hover,input:focus,textarea:hover,textarea:focus,select:hover,select:focus":{
        "border":"1px solid #777"
    },
    "input:focus-visible,textarea:focus-visible,select:focus-visible":{
        "outline":"none"
    },
    "div:focus-visible":{
        "outline":"2px dashed darkorange"
    },
    "form":{
        "padding":"0px",
        "margin":"0px"
    },
    "button":{
        "background-color":"#fffa",
        "border":"1px solid black"
    },
    "footer":{
    },
    "section":{
        "padding":"16px",
        "margin":"16px 0",
        "background-color":"#fffa",
        "border":"1px solid #7777"
    },
    "section:hover":{
        "border-color":"#777"
    },
    "article":{
        "padding":"0px",
        "margin":"0 0 16px 0"
    },
    "figure":{
        "padding":"0px",
        "margin":"0px"
    },
    "figcaption":{
        "color":"cornflowerblue",
        "font-style":"italic"
    },
    "blockquote":{
        "border":" 1px solid #bfbfbfed",
        "padding":"16px",
        "margin":"16px 0",
        "border-radius":"16px",
        "background":"#dddddd75"
    },
    
    "table":{
        "margin":"0px",
        "border-width":"2px",
        "border-style":"solid",
        "border-color":"#222",
        "border-collapse":"collapse",
        "width":"100%"
    },
    "tr:nth-of-type(2n)":{
        "background":"#0000001c"
    },
    "td,th":{
        "color":"#222",
        "padding":"4px"
    },
    "th":{
        "color":"#fff",
        "text-align":"left",
        "background-color":"#0000009c"
    },
    
    ".grid":{
        "transition":"all 0.3s ease",
        "display":"grid",
        "gap":"0px"
    },
    ".grid-c1":{"grid-column":"1"},
    ".grid-c2":{"grid-column":"2"},
    ".grid-c3":{"grid-column":"3"},
    ".grid-r1":{"grid-row":"1"},
    ".grid-r2":{"grid-row":"2"},
    ".grid-r3":{"grid-row":"3"},
    ".grid-top":{
        "grid-column":"1 / 3",
        "grid-row":"1"
    },
    ".grid-ban":{
        "grid-column":"1",
        "grid-row":"1"
    },
    ".grid-left":{
        "grid-column":"1",
        "grid-row":"2"
    },
    ".grid-body":{
        "grid-column":"2",
        "grid-row":"1 / 2"
    },
    ".grid-right":{
        "grid-column":"3",
        "grid-row":"2"
    },
    ".grid-footer":{
        "grid-column":"1",
        "grid-row":"3"
    },

    "tabs":{
        "line-height":"20px"
    },
    ".tabs span":{
        "transition":"all 0.3s ease",
        "padding":"2px 7px",
        "border-radius":"6px 6px 0px 0px",
        "color":"#333333",
        "background-color":"#e9e7e8",
        "box-shadow":"0px 0px 4px -3px #777777"
    },
    ".tabs span:hover":{
        "color":"#171717",
        "background-color":"#ffffff"
    },
    ".tabs span.active":{
        "color":"#171717",
        "background-color":"#ffffff"
    },
    ".tabs span.space":{
        "padding":"2px 0px",
        "border-width":"0 0 1px 0",
        "background":"linear-gradient(to bottom,rgba(0,0,0,0),rgba(68,68,68,0.5))"
    },

    ".frame-white":{
        "font-size":"big",
        "padding":"8px 16px",
        "margin":"4px",
        "background-color":"#ffffff5e",
        "border-color":"#6c6c6ca1",
        "border-style":"solid",
        "border-width":"1px"
    },
    ".frame-blue":{
        "font-size":"big",
        "padding":"8px 16px",
        "margin":"4px",
        "background-color":"#0064ff4a",
        "border-color":"#0064ff",
        "border-style":"solid",
        "border-width":"1px"
    },
    ".frame-red":{
        "font-size":"big",
        "padding":"8px 16px",
        "margin":"4px",
        "background-color":"#dc143c4a",
        "border-color":"#dc143c",
        "border-style":"solid",
        "border-width":"1px"
    },
    ".frame-green":{
        "font-size":"big",
        "padding":"8px 16px",
        "margin":"4px",
        "background-color":"#9acd3273",
        "border-color":"#608e00",
        "border-style":"solid",
        "border-width":"1px"
    },
    
    ".btn,.btsav,.btdel":{
        "transition":"all 0.3s ease",
        "font-size":"small",
        "padding":"1px 3px",
        "background-color":"#ffffff7d",
        "border-width":"1px",
        "border-color":"grey",
        "border-style":"solid",
        "line-height":"24px",
        "color":"#444"
    },
    ".btn:hover":{
        "background-color":"white",
        "border-color":"black"
    },
    ".btsav":{
        "background-color":"#0064ff4a",
        "border-color":"#0064ff",
        "color":"#222"
    },
    ".btsav:hover":{
        "background-color":"#0064ff",
        "border-color":"#0064ff",
        "color":"#fff !important"
    },
    ".btdel":{
        "background-color":"#dc143c4a",
        "border-color":"#dc143c",
        "color":"#222"
    },
    ".btdel:hover":{
        "background-color":"#dc143c",
        "border-color":"#dc143c",
        "color":"#fff !important"
    },
    ".editable":{
        "outline-width":"2px",
        "outline-style":"dotted",
        "outline-color":"#eee"
    },
    ".editable:hover":{
        "outline-color":"#777"
    },
    ".editable.active":{
        "outline-color":"red"
    },

    ".console":{
        "font-family":"Lucida Console",
        "word-wrap":"break-word",
        "padding":"16px",
        "background-color":"#222",
        "color":"#fff"
    },
    "scroll":{
        "max-height":"400px",
        "min-width":"140px",
        "overflow-y":"auto",
        "overflow-x":"hidden",
        "word-wrap":"break-word",
        "scrollbar-width":"thin"
    },
    ".block-inline":{
        "display":"inline-block",
        "border":" 1px solid #ffffffe5",
        "padding":"5px 10px",
        "margin":"16px 0",
        "border-radius":"16px",
        "background":"#dddddd75"
    },
    "cols":{
        "columns":"auto 250px",
        "margin-top":"16px"
    },
    ".infos":{
        "font-size":"small"
    },
    ".infos span":{
        "padding":"2px"
    },
    ".info":{
        "padding":"2px 0",
        "color":"#c700ff"
    },
    ".small":{
        "font-size":"small"
    },
    ".right":{
        "float":"right"
    },
    ".ico":{
        "font-size":"normal"
    },
    
    "@media only screen and (max-width: 720px)":{
        ".react":{
            "display":"none"
        },
        ".grid":{
            "grid-template-columns":"50px auto"
        }
    }
}